<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>WarehouseServlet</servlet-name>
        <servlet-class>controller.WarehouseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BinServlet</servlet-name>
        <servlet-class>controller.BinServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ZoneServlet</servlet-name>
        <servlet-class>controller.ZoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WarehouseServlet</servlet-name>
        <url-pattern>/warehouseservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BinServlet</servlet-name>
        <url-pattern>/binservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ZoneServlet</servlet-name>
        <url-pattern>/zoneservlet</url-pattern>
    </servlet-mapping>
</web-app>
